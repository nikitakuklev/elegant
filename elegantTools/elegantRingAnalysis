proc FillDynamicApertureErrorsFrame {widget args} {
    global dynamicApertureErrors templateDirectory

    pack [ttk::frame $widget.f1] -anchor nw
    grid [ttk::label $widget.f1.l1 -text "Maximum x coordinate (m):"] -row 1 -column 1 -sticky w
    APSLabeledEntry .le1 -parent $widget.f1 \
      -label "" -width 20 -textVariable dynamicApertureErrors(xMaximum) \
      -contextHelp \
      "Enter the maximum x coordinate for the dynamic aperture search" \
      -gridPack "-row 1 -column 2 -sticky w"
    grid [ttk::label $widget.f1.l2 -text "Maximum y coordinate (m):"] -row 2 -column 1 -sticky w
    APSLabeledEntry .le2 -parent $widget.f1 \
      -label "" -width 20 -textVariable dynamicApertureErrors(yMaximum) \
      -contextHelp \
      "Enter the maximum y coordinate for the dynamic aperture search" \
      -gridPack "-row 2 -column 2 -sticky w"
    grid [ttk::label $widget.f1.l3 -text "Number of steps along lines:"] -row 3 -column 1 -sticky w
    APSLabeledEntry .le3 -parent $widget.f1 \
      -label "" -width 20 -textVariable dynamicApertureErrors(steps) \
      -contextHelp \
      "Enter the number of points into which to divide the line." \
      -gridPack "-row 3 -column 2 -sticky w"
    grid [ttk::label $widget.f1.l4 -text "Number of divisions:"] -row 4 -column 1 -sticky w
    APSLabeledEntry .le4 -parent $widget.f1 \
      -label "" -width 20 -textVariable dynamicApertureErrors(splits) \
      -contextHelp \
      "Enter the number of times to divide the interval." \
      -gridPack "-row 4 -column 2 -sticky w"
    grid [ttk::label $widget.f1.l5 -text "Lines:"] -row 5 -column 1 -sticky w
    APSRadioButtonFrame .rb1 -parent $widget.f1 -label "" \
      -orientation horizontal -variable dynamicApertureErrors(lines) \
      -valueList "1 3 5 7 9 11 15 19 23 27 31" \
      -buttonList "1 3 5 7 9 11 15 19 23 27 31" -contextHelp \
      "Number of lines to search on starting at the origin." \
      -gridPack "-row 5 -column 2 -sticky w"
    grid [ttk::label $widget.f1.l6 -text "Fractional momentum offset:"] -row 6 -column 1 -sticky w
    APSLabeledEntry .le5a -parent $widget.f1 \
      -label "" -width 20 -textVariable dynamicApertureErrors(delta) \
      -contextHelp "Enter the fractional momentum offset." \
      -gridPack "-row 6 -column 2 -sticky w"
    grid [ttk::label $widget.f1.l7 -text "Number of turns:"] -row 7 -column 1 -sticky w
    APSLabeledEntry .le6 -parent $widget.f1 \
      -label "" -width 20 -textVariable dynamicApertureErrors(turns) \
      -contextHelp \
      "Enter the number of turns to track."  \
      -gridPack "-row 7 -column 2 -sticky w"

    grid [ttk::label $widget.f1.l8 -text "Run suffix:"] -row 8 -column 1 -sticky w
    APSLabeledEntry .le7 -parent $widget.f1 \
      -label "" -width 20 -textVariable dynamicApertureErrors(userSuffix) \
      -contextHelp "Enter a suffix for this set of runs." \
      -gridPack "-row 8 -column 2 -sticky w"

    grid [ttk::label $widget.f1.l9 -text "Include apertures:"] -row 9 -column 1 -sticky w
    APSRadioButtonFrame .rb2 -parent $widget.f1 \
      -label "" -orientation horizontal -buttonList {Yes No} -valueList {1 0} \
      -variable dynamicApertureErrors(includeApertures) -contextHelp \
      "Choose whether to include the effects of apertures that are defined in the beamline." \
      -gridPack "-row 9 -column 2 -sticky w"

    grid [ttk::label $widget.f1.l10 -text "Number of cores per job:"] -row 10 -column 1 -sticky w
    APSLabeledEntry .nproc -parent $widget.f1 \
	-label "" -width 20 \
	-textVariable dynamicApertureErrors(nCores) \
	-contextHelp "Enter the number of cores per job.  If 1, then serial elegant is used." \
      -gridPack "-row 10 -column 2 -sticky w"

    grid [ttk::label $widget.f1.l11 -text "Aperture file:"] -row 11 -column 1 -sticky w
    APSLabeledEntry .leaf -parent $widget.f1 \
      -label "" -width 80 -textVariable dynamicApertureErrors(apertureFile) \
      -fileSelectButton 1 -buttonsOnLeft 0 -fileSelectPattern *.sdds \
      -contextHelp \
      "Enter the name of a file containing aperture data." \
      -gridPack "-row 11 -column 2 -sticky w"

    set dynamicApertureErrors(template) [file join $templateDirectory $dynamicApertureErrors(template)]
    grid [ttk::label $widget.f1.l12 -text "Template file:"] -row 12 -column 1 -sticky w
    APSLabeledEntry .le8 -parent $widget.f1 \
        -label "" -width 80 -textVariable dynamicApertureErrors(template) \
        -contextHelp "Enter the name of the template file for the runs." \
      -gridPack "-row 12 -column 2 -sticky w"
    
    pack [ttk::frame $widget.f2] -anchor nw
    APSButton .bu1 -parent $widget.f2 \
      -text "Run" -command "Run -computation DynamicApertureErrors" -width ""
    APSButton .bu1a -parent $widget.f2 \
      -text "Process" -command "Run -computation DynamicApertureErrors -processOnly 1" -width ""
    APSButton .bu2 -parent $widget.f2 \
      -text "Display" -command \
      {Display -type DynamicApertureErrors} -width ""
    global apsAbortElegantRun
    set apsAbortElegantRun(APSElegantDynamicApertureErrorsPP) 0
    APSButton .abort -parent $widget.f2 -text "Abort" \
      -command "set apsAbortElegantRun(APSElegantDynamicApertureErrorsPP) 1" \
      -contextHelp "click to abort any running processes." -width ""
}


proc FillKickApertureErrorsFrame {widget args} {
    global kickApertureErrors templateDirectory

    set row 1
    pack [ttk::frame $widget.f1] -anchor nw
    grid [ttk::label $widget.f1.l$row -text "Minimum kick (mrad):"] -row $row -column 1 -sticky w
    APSLabeledEntry .le1 -parent $widget.f1 \
      -label "" -width 20 -textVariable kickApertureErrors(minimumKick) \
      -contextHelp \
      "Enter the maximum x plane kick for the kick aperture search" \
      -gridPack "-row $row -column 2 -sticky w"
    incr row
    grid [ttk::label $widget.f1.l$row -text "Maximum kick (mrad):"] -row $row -column 1 -sticky w
    APSLabeledEntry .le2 -parent $widget.f1 \
      -label "" -width 20 -textVariable kickApertureErrors(maximumKick) \
      -contextHelp \
      "Enter the maximum x plane kick for the kick aperture search" \
      -gridPack "-row $row -column 2 -sticky w"
    incr row
    grid [ttk::label $widget.f1.l$row -text "Number of steps:"] -row $row -column 1 -sticky w
    APSLabeledEntry .le3 -parent $widget.f1 \
      -label "" -width 20 -textVariable kickApertureErrors(steps) \
      -contextHelp \
      "Enter the number of points into which to divide the range." \
      -gridPack "-row $row -column 2 -sticky w"
    incr row
    grid [ttk::label $widget.f1.l$row -text "Number of turns:"] -row $row -column 1 -sticky w
    APSLabeledEntry .le4 -parent $widget.f1 \
      -label "" -width 20 -textVariable kickApertureErrors(turns) \
      -contextHelp \
      "Enter the number of turns to track."  \
      -gridPack "-row $row -column 2 -sticky w"
    incr row

    grid [ttk::label $widget.f1.l$row -text "Number of particles:"] -row $row -column 1 -sticky w
    APSLabeledEntry .le5 -parent $widget.f1 \
      -label "" -width 20 -textVariable kickApertureErrors(particles) \
      -contextHelp \
      "Enter the number of particles to track."   \
      -gridPack "-row $row -column 2 -sticky w"
    incr row

    grid [ttk::label $widget.f1.l$row -text "Emittance ratio:"] -row $row -column 1 -sticky w
    APSLabeledEntry .le6 -parent $widget.f1 \
      -label "" -width 20 -textVariable kickApertureErrors(emittanceRatio) \
      -contextHelp \
      "Enter the emittance ratio (ey/ex)"  \
      -gridPack "-row $row -column 2 -sticky w"
    incr row

    grid [ttk::label $widget.f1.l$row -text "Element name for kick location:"] -row $row -column 1 -sticky w
    APSLabeledEntry .le7 -parent $widget.f1 \
      -label "" -width 20 -textVariable kickApertureErrors(elementName) \
      -contextHelp "Enter name of the element at which to put initial kick."  \
      -gridPack "-row $row -column 2 -sticky w"
    incr row

    grid [ttk::label $widget.f1.l$row -text "Run suffix:"] -row $row -column 1 -sticky w
    APSLabeledEntry .le9 -parent $widget.f1 \
      -label "" -width 20 -textVariable kickApertureErrors(userSuffix) \
      -contextHelp "Enter a suffix for this set of runs."  \
      -gridPack "-row $row -column 2 -sticky w"
    incr row

    grid [ttk::label $widget.f1.l$row -text "Include apertures:"] -row $row -column 1 -sticky w
    APSRadioButtonFrame .rb2 -parent $widget.f1 \
      -label "" -orientation horizontal -buttonList {Yes No} -valueList {1 0} \
      -variable kickApertureErrors(includeApertures) -contextHelp \
      "Choose whether to include the effects of apertures that are defined in the beamline."  \
      -gridPack "-row $row -column 2 -sticky w"
    incr row

    grid [ttk::label $widget.f1.l$row -text "Aperture file:"] -row $row -column 1 -sticky w
    APSLabeledEntry .leaf -parent $widget.f1 \
      -label "" -width 80 -textVariable kickApertureErrors(apertureFile) \
      -fileSelectButton 1 -buttonsOnLeft 0 -fileSelectPattern *.sdds \
      -contextHelp \
      "Enter the name of a file containing aperture data."  \
      -gridPack "-row $row -column 2 -sticky w"
    incr row

    grid [ttk::label $widget.f1.l$row -text "Template file:"] -row $row -column 1 -sticky w
    APSLabeledEntry .le11 -parent $widget.f1 \
        -label "" -width 80 -textVariable kickApertureErrors(template) \
        -contextHelp "Enter the name of the template file for the runs."  \
      -gridPack "-row $row -column 2 -sticky w"
    incr row

    grid [ttk::label $widget.f1.l$row -text "Number of processors per job:"] -row $row -column 1 -sticky w
    APSLabeledEntry .le12 -parent $widget.f1 \
	-label "" -width 20 -textVariable kickApertureErrors(processors) \
	-contextHelp "Enter the number of processors per job."  \
      -gridPack "-row $row -column 2 -sticky w"
    incr row

    pack [ttk::frame $widget.f2] -anchor nw
    APSButton .bu1 -parent $widget.f2 \
      -text "Run" -command "Run -computation KickApertureErrors" -width ""
    #APSButton .bu1a -parent $widget \
    #  -text "Process" -command "Run -computation KickApertureErrors -processOnly 1"
    APSButton .bu2 -parent $widget.f2 \
      -text "Display" -command {Display -type KickApertureErrors} -width ""
    global apsAbortElegantRun
    set apsAbortElegantRun(APSElegantKickApertureErrorsPP) 0
    APSButton .abort -parent $widget.f2 -text "Abort" \
      -command "set apsAbortElegantRun(APSElegantKickApertureErrorsPP) 1" \
      -contextHelp "click to abort any running processes." -width ""
}



