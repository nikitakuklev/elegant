      -variableList [list collectiveEffects(rfVoltage0) collectiveEffects(rfVoltage1)]
    APSLabeledEntryFrame .emitRatio -parent $widget.f1 -type real \
      -label "Emittance ratio limits (ey/ex):                                        " \
      -width 10 -orientation horizontal \
