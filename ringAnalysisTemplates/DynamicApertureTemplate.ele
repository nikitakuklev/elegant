&transmute_elements
        name = *, type = WATCH, 
        new_type = DRIF
&end

&transmute_elements
        name = *, type = MAXAMP,
        new_type = DRIF,
	ignore = <includeApertures>
&end
&transmute_elements
        name = *, type = ?COL,
        new_type = DRIF,
	ignore = <includeApertures>
&end
&transmute_elements
        name = *, type = SCRAPER,
        new_type = DRIF,
	ignore = <includeApertures>
&end

&run_setup
	lattice = <kickLatticeFile>,
	use_beamline = <kickBeamline>
	p_central = "{rpnl <momentum> mev /}",
        magnets = %s.mag,
        rootname = <mainDirectory>/<mainPrefix>DynamicAperture,
        final = %s.fin,
        semaphore_file = %s.done,
        tracking_updates = 0
&end

&load_parameters
        filename_list = " <parameterFileList>",
        change_defined_values = 1,
        allow_missing_elements = 1,
        allow_missing_parameters = 1,
        allow_missing_files = 1,
&end

&alter_elements name=*, type=*SBEN*, item=EDGE_ORDER, value=1 &end

&run_control
        n_passes = <turns>,
&end

&find_aperture
        output = %s.aper,
        boundary = %s.abnd,
        xmin = <xMinimum>, xmax=<xMaximum>,
        ymax = <yMaximum>,
        mode = many-particle,
        nx = <xPoints>,
        ny = <yPoints>,
        verbosity = 1,
&end

